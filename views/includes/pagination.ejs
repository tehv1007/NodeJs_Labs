<section class="pagination">
    <% if (currentPage !== 1 && previousPage !== 1) { %>
      <a href="?page=1&rowpage=<%=ITEMS_PER_PAGE%>">1</a>
    <% } %> <% if (hasPreviousPage) { %>
      <a href="?page=<%= previousPage %>&rowpage=<%=ITEMS_PER_PAGE%>"><%= previousPage %></a>
    <% } %>
      <a href="?page=<%= currentPage %>&rowpage=<%=ITEMS_PER_PAGE%>" class="active"
      ><%= currentPage %></a
    >
    <% if (hasNextPage) { %>
      <a href="?page=<%= nextPage %>&rowpage=<%=ITEMS_PER_PAGE%>"><%= nextPage %></a>
    <% } %> <% if (lastPage !== currentPage && nextPage !== lastPage) { %>
      <a href="?page=<%= lastPage %>&rowpage=<%=ITEMS_PER_PAGE%>"><%= lastPage %></a>
    <% } %>
  </section>